<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>RentalBookBook</title>
</head>
<body>
  <a href="/">Home</a>
  <h1>List of Authors</h1>
  <table border="1">
    <thead>
      <th>NAME</th>
      <th>AGE</th>
      <th>LIST OF BOOKS</th>
    </thead>
    <tbody>
      <% result.forEach((el) => { %>
        <tr>
          <td><%= el.fullName %> </td>
          <td><%= el.age %> </td>
          <td><a href="/authors/<%= el.id %>">Books List</a></td>
        </tr>
      <% }) %> 
    </tbody>
  </table>
</body>
</html>